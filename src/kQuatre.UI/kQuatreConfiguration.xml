<?xml version="1.0" encoding="utf-8"?>
<kQuatreConfiguration>
	<ExcelFile>
		<FireworkName>E2</FireworkName>
		<FireworkDataRow>5</FireworkDataRow>
    <FireworkSheetNumber>0</FireworkSheetNumber>
  </ExcelFile>
  <Transceiver>
    <Baudrate>115200</Baudrate>
    <!--<RetryPingTransceiver>3</RetryPingTransceiver>-->
    <!-- Temps d'attente de réception en ms d'un ACK en provenance d'un récepteur une fois une trame envoyée -->
    <AckTimeOut>200</AckTimeOut>
    <!-- Temps d'attente total en ms envoi d'une trame + réception d'une trame -->
    <TotalTimeout>500</TotalTimeout>  
    <!-- Si une frame tombe en timout ou ko alors nb de fois ou on retentera d'envoyer la frame -->
    <RetryFrameEmission>2</RetryFrameEmission>    
    <Address>0</Address>    
</Transceiver>
	<Receptors>
		<Receptor name="Boitier 1" address="1" nbOfChannels="16" />
		<Receptor name="Boitier 2" address="2" nbOfChannels="16" />
		<Receptor name="Boitier 3" address="3" nbOfChannels="16" />
		<Receptor name="Boitier 4" address="4" nbOfChannels="16" />
  </Receptors>
</kQuatreConfiguration>